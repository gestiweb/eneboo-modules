		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>20</length>
	</field>

	<field>
		<name>email</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","E-mail")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
	</field>

	<field>
		<name>rmercantil</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Registro Mercantil")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
	</field>

	<field>
		<name>lopd</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","LOPD")</alias>
		<null>true</null>
