	<area>F</area>
	<areaname>QT_TRANSLATE_NOOP("FLWidgetApplication","Area de Facturación")</areaname>
	<version>2.2</version>
	<icon>flfacthora.xpm</icon>
	<flversion>2.2</flversion>
	<dependencies>
		<dependency>flfactppal</dependency>
