<!-- Cantidad de transferencias de stock-->        <alias>QT_TRANSLATE_NOOP("MetaData","Cantidad T.S.")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>10</partI>
        <partD>2</partD>
        <default>0</default>
        <editable>false</editable>
        <visiblegrid>false</visiblegrid>
    </field>
    <field>
        <name>cantidadtpv</name>
<!-- Cantidad de ventas de TPV de cliente-->        <alias>QT_TRANSLATE_NOOP("MetaData","Cantidad TPV")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>10</partI>
        <partD>2</partD>
        <default>0</default>
        <editable>false</editable>
        <visiblegrid>false</visiblegrid>
    </field>
    <field>
        <name>fechaultreg</name>
        <alias>QT_TRANSLATE_NOOP("MetaData","Fecha")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>date</type>
        <editable>false</editable>
    </field>
    <field>
        <name>horaultreg</name>
<!-- hora de la última regularización -->        <alias>QT_TRANSLATE_NOOP("MetaData","Hora")</alias>
        <null>true</null>
        <pk>false</pk>
        <type>time</type>
        <editable>false</editable>
    </field>
    <field>
