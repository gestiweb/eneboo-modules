        <partI>10</partI>
        <partD>2</partD>
        <default>0</default>
        <editable>false</editable>
        <visiblegrid>false</visiblegrid>
    </field>
    <field>
        <name>cantidadfp</name>
<!-- Cantidad de facturas de proveedor-->        <alias>QT_TRANSLATE_NOOP("MetaData","Cantidad F.P.")</alias>
        <null>false</null>
        <pk>false</pk>
        <type>double</type>
        <partI>10</partI>
        <partD>2</partD>
        <default>0</default>
        <editable>false</editable>
        <visiblegrid>false</visiblegrid>
    </field>
