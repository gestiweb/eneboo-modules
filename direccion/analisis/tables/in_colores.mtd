		<type>string</type>
		<length>3</length>
		<regexp>^([01][0-9][0-9]|2[0-4][0-9]|25[0-5])$</regexp>
		<default>000</default>
	</field>
	
	<field>
		<name>b</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Azul")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>double</type>
		<type>string</type>
		<length>3</length>
		<regexp>^([01][0-9][0-9]|2[0-4][0-9]|25[0-5])$</regexp>
		<default>000</default>
	</field>
	
	<field>
		<name>rgb</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Color")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>11</length>
	</field>
