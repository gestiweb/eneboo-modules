<!DOCTYPE TMD>
<TMD>
	<name>in_dimagente</name>
	<!--Dimensión de agentes-->
	<alias>QT_TRANSLATE_NOOP("MetaData","Dimensión de agente")</alias>

	<in_Dimension name="Agente">
		<in_Hierarchy hasAll="true" allMemberName="All agents" >
			<in_Level column="codagente" uniqueMembers="true" ordinalColumn="nombre" nameColumn="nombre"/>
		</in_Hierarchy>
	</in_Dimension>
	
	<field in_property="true">
		<name>codagente</name>
		<!--Código del agente comercial-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Agente")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>string</type>
		<length>10</length>
		
		<relation>
			<table>in_h_ventas</table>
			<field>dim_codagente</field>
			<card>1M</card>
		</relation>
	</field>
	<field>
		<name>nombreap</name>
		<alias>QT_TRANSLATE_NOOP("MetaData","Nombre")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>150</length>
	</field>
</TMD>
