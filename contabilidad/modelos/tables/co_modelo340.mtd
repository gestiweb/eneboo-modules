		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>9</length>
	</field>
 	
	<field>
		<name>codelectronico</name>
		 <!--Código electrónico autoliquidación IVA-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Cod. electrónico)</alias>
		<null>true</null>
		<pk>false</pk>
