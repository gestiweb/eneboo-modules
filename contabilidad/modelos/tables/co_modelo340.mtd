		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
		<default>false</default>
	</field>
	
	<field>
		<name>sustitutiva</name>
		<!--Declaración sustitutiva-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Sustitutiva")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>bool</type>
		<default>false</default>
	</field>
	
