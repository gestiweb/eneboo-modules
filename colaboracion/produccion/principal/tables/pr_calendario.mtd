<!DOCTYPE TMD>
<TMD>
	
	<name>pr_calendario</name>
	<!--Dias del año y horario-->
	<alias>QT_TRANSLATE_NOOP("MetaData","Calendario Laboral")</alias>
	
	<field>
		<name>fecha</name>
		<!--Identificador-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Fecha")</alias>
		<null>false</null>
		<pk>true</pk>
		<type>date</type>
	</field>
	
	<field>
		<name>descripcion</name>
		<!--Descripción del dia-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Descripción")</alias>
		<null>false</null>
		<pk>false</pk>
		<type>string</type>
		<length>100</length>
		<visible>true</visible>
	</field>	
	
	<field>
		<name>codhorario</name>
		<!--Horario-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Horario")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>string</type>
		<length>8</length>
				
		<relation>
			<table>pr_horarios</table>
			<field>codhorario</field>
			<card>M1</card>
		</relation>
	</field>
	
	<field>
		<name>horaentradamanana</name>
		<!--Hora a la que se comienza a trabajar por la mañana-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Hora de entrada mañana")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>time</type>
		<default>null</default>
	</field>
	
	<field>
		<name>horasalidamanana</name>
		<!--Hora a la que se deja de trabajar por la mañana-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Hora de salida mañana")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>time</type>
		<default>null</default>
	</field>
	
	<field>
		<name>horaentradatarde</name>
		<!--Hora a la que se comienza a trabajar por la tarde-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Hora de entrada tarde")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>time</type>
		<default>null</default>
	</field>
	
	<field>
		<name>horasalidatarde</name>
		<!--Hora a la que se deja de trabajar por la tarde-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Hora de salida tarde")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>time</type>
		<default>null</default>
	</field>
	
	<field>
		<name>tiempo</name>
		<!--Tiempo en milisegundos-->
		<alias>QT_TRANSLATE_NOOP("MetaData","Tiempo")</alias>
		<null>true</null>
		<pk>false</pk>
		<type>double</type>
		<partI>10</partI>
		<visiblegrid>false</visiblegrid>
	</field>
</TMD>