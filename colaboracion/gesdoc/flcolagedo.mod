<!DOCTYPE MODULE>
<MODULE>
	<name>flcolagedo</name>
	<alias>QT_TRANSLATE_NOOP("FLWidgetApplication","Gestión documental")</alias>
	<area>L</area>
	<areaname>QT_TRANSLATE_NOOP("FLWidgetApplication","Área de Colaboración")</areaname>
	<version>2.3</version>
	<icon>flcolagedo.xpm</icon>
